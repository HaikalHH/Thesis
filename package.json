{
  "name": "@haikal/react-pdf-viewer",
  "version": "1.0.0",
  "description": "React TypeScript library untuk preview PDF di Next.js menggunakan PDF.js",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "build:tsc": "tsc --project tsconfig.lib.json",
    "dev": "pnpm --dir examples dev",
    "dev:examples": "pnpm --dir examples dev",
    "build:examples": "pnpm --dir examples build",
    "link:examples": "pnpm link --global && pnpm --dir examples link @haikal/react-pdf-viewer",
    "prepublishOnly": "pnpm build"
  },
  "keywords": [
    "react",
    "nextjs",
    "pdf",
    "viewer",
    "preview",
    "typescript",
    "pdfjs"
  ],
  "author": "Haikal",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/haikal/react-pdf-viewer.git"
  },
  "peerDependencies": {
    "next": "^14.0.0 || ^15.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "dependencies": {
    "pdfjs-dist": "^4.0.379"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "esbuild": "^0.19.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.3"
  },
  "files": [
    "dist",
    "lib",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "packageManager": "pnpm@10.15.0+sha512.486ebc259d3e999a4e8691ce03b5cac4a71cbeca39372a9b762cb500cfdf0873e2cb16abe3d951b1ee2cf012503f027b98b6584e4df22524e0c7450d9ec7aa7b"
}
